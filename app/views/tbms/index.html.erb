<p id="notice"><%= notice %></p>

<h1>Tbms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Add</th>
      <th>Kin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tbms.each do |tbm| %>
      <tr>
        <td><%= tbm.name %></td>
        <td><%= tbm.add %></td>
        <td><%= tbm.kin %></td>
        <td><%= link_to 'Show', tbm %></td>
        <td><%= link_to 'Edit', edit_tbm_path(tbm) %></td>
        <td><%= link_to 'Destroy', tbm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tbm', new_tbm_path %>
